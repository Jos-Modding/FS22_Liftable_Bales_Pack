        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut01" length="1.5" diameter="0.02" straighteningFactor="1.5">
            <objectChange node="hydraulicOut01_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicIn"  node="hydraulicIn02"  length="1.5" diameter="0.02" straighteningFactor="1.5">
            <objectChange node="hydraulicIn02_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut02" length="1.5" diameter="0.02" straighteningFactor="1.5">
            <objectChange node="hydraulicOut02_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
    </connectionHoses>

    <dynamicallyLoadedParts>
        <dynamicallyLoadedPart node="1|0" linkNode="hydraulicIn01_connector"  filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|0" linkNode="hydraulicOut01_connector"  filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|0" linkNode="hydraulicIn02_connector"  filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|0" linkNode="hydraulicOut02_connector"  filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="0" linkNode="camera_02_01" filename="$data/shared/assets/camera_02.i3d" rotationNode="0|0" rotation="10 0 0"/>
    </dynamicallyLoadedParts>

    <powerTakeOffs>
        <input inputNode="pto" detachNode="ptoDetachNode" inputAttacherJointIndices="1" aboveAttacher="true" filename="$data/shared/assets/powerTakeOffs/walterscheidWWZ.xml"/>
        <local startNode="powerTakeOff01" endNode="powerTakeOff01Ref" />
        <local startNode="powerTakeOff02" endNode="powerTakeOff02Ref" />
    </powerTakeOffs>

    <powerConsumer ptoRpm="400" neededMinPtoPower="70" neededMaxPtoPower="80"/>

    <groundReferenceNodes>
        <groundReferenceNode node="groundReferenceNode" threshold="0.5"/>
    </groundReferenceNodes>

    <workAreas>
