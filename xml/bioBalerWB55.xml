            <animationNode class="ScrollingAnimation" node="chain02"  scrollSpeed="-3" scrollLength="10" shaderParameterComponent="2" turnOnFadeTime="1" turnOffFadeTime="1"/>
            <animationNode class="ScrollingAnimation" node="bigchain"  scrollSpeed="-0.4" scrollLength="10" shaderParameterComponent="1" turnOnFadeTime="1" turnOffFadeTime="1"/>
        </animationNodes>
    </baler>

    <cutter fruitTypes="poplar maize wheat barley oat canola sunflower soybean sorghum" fruitTypeConverter="forageHarvester">
        <fillEffect>
            <effectNode effectClass="ParticleEffect" effectNode="emitterShapePoplarChips" particleType="CRUSHER_WOOD" emitCountScale="20.0" useCuttingWidth="false"/>
            <effectNode effectClass="ParticleEffect" effectNode="emitterShapeSmoke"       particleType="SMOKE" materialType="smokeParticle" emitCountScale="0.7" useCuttingWidth="false"/>
        </fillEffect>
    </cutter>

    <combine fillUnitIndex="1" allowThreshingDuringRain="true">
        <swath available="false"/>
        <chopper available="false"/>
    </combine>

    <animations>
        <animation name="unloadBale">
            <part node="backDoor" startTime="0" endTime="2" startRot="0 0 0" endRot="75 0 0" />
            <part node="baleUnloader" startTime="2.0" endTime="2.5" startRot="10 0 0" endRot="-5 0 0"  />
            <part node="baleUnloader" startTime="4.5" endTime="5" startRot="-5 0 0" endRot="10 0 0"  />

            <part node="baleNode" startTime="1"   endTime="2"    startTrans="0.004 1.421 -0.179" endTrans="0.004 1.113 -0.703" startRot="0.0 0.0 0.0" endRot="-53.0 0.0 0.0" />
            <part node="baleNode" startTime="2"   endTime="2.5"  startTrans="0.004 1.113 -0.703" endTrans="0.004 0.959 -0.965" startRot="-53.0 0.0 0.0" endRot="-84.74 0.0 0.0" />
            <part node="baleNode" startTime="2.5" endTime="3"    startTrans="0.004 0.959 -0.965" endTrans="0.004 0.839 -1.354" startRot="-84.74 0.0 0.0" endRot="-124.447 0.0 0.0" />
            <part node="baleNode" startTime="3"   endTime="3.5"  startTrans="0.004 0.839 -1.354" endTrans="0.004 0.759 -1.719" startRot="-124.447 0.0 0.0" endRot="-164.154 0.0 0.0" />
            <part node="baleNode" startTime="3.5" endTime="3.7"  startTrans="0.004 0.759 -1.719" endTrans="0.004 0.759 -1.861" startRot="-164.154 0.0 0.0" endRot="-175.074 0.0 0.0" />
            <part node="baleNode" startTime="3.7" endTime="5"    startTrans="0.004 0.759 -1.861" endTrans="0.004 0.759 -2.563" startRot="-175.074 0.0 0.0" endRot="-246.056 0.0 0.0" />

            <sound template="defaultHydraulicSound" startTime="0.01" endTime="1.99" volumeScale="1.5" pitchScale="0.8" />
            <sound template="foldStartMed01"        startTime="0.01"                volumeScale="2.2" pitchScale="1.5" />
            <sound template="metalOpen"             startTime="1.8"  direction="1"  volumeScale="2.8" pitchScale="0.8" />
            <sound template="sheetImp02"            startTime="1.8"  direction="1"  volumeScale="2.7" pitchScale="0.8" />
            <sound template="BALER_EJECT"           startTime="0.01" endTime="2"    volumeScale="2.5" pitchScale="1.5" />
        </animation>

        <animation name="closeDoor" resetOnStart="false">
            <part node="backDoor" startTime="0" endTime="2" startRot="60 0 0" endRot="0 0 0" />
            <part node="baleNode" startTime="0" endTime="2" endRot="0 0 0" endTrans="0.004 1.421 -0.179" />

            <sound template="defaultHydraulicSound" startTime="0.01" endTime="1.99" volumeScale="2.5" pitchScale="0.8" />
            <sound template="sheetImp02"            startTime="1.99"                volumeScale="2"   pitchScale="0.8" />
            <sound template="metalClose"            startTime="1.99"                volumeScale="3"   pitchScale="0.6" />
            <sound template="clackVar5"             startTime="0.01"                volumeScale="0.7" pitchScale="1.5" />
        </animation>

        <animation name="baleFillAnimation">
            <part node="baleScaleNode" startTime="0" endTime="1" startTrans="0 -0.569 0.479" endTrans="0 0 0" />
        </animation>

        <animation name="foldWheels">
            <part node="axis" startTime="0" endTime="2" startRot="45 0 0" endRot="5 0 0" />
            <part node="wheelsRot" startTime="0" endTime="2" startRot="-45 0 0" endRot="-5 0 0" />

            <sound template="defaultHydraulicSound" startTime="0.01" endTime="1.99" volumeScale="1.3" pitchScale="1.2" fadeIn="0.1" fadeOut="0.2"/>
            <sound template="clackVar5"             startTime="1.99" direction="1"  volumeScale="0.7" pitchScale="0.7" />
            <sound template="sheetImp02"            startTime="1.99" direction="1"  volumeScale="1.7" pitchScale="0.7" />
            <sound template="sheetImp05"            startTime="1.99" direction="1"  volumeScale="0.7" pitchScale="0.7" />
            <sound template="clackVar6"             startTime="0.01" direction="-1" volumeScale="0.9" pitchScale="0.8" />
            <sound template="cylinderPunch"         startTime="0.01" direction="-1" volumeScale="0.3" pitchScale="1.2" />
        </animation>

        <animation name="moveSupport">
            <part node="supportFoot"      startTime="0" endTime="0.2" startTrans="0 0.15 -0.003" endTrans="0 -0.241 -0.003"/>
            <part node="opticalSupportFoot" startTime="0" endTime="0.199" visibility="false"/>
            <part node="opticalSupportFoot" startTime="0.199" endTime="0.2" visibility="true"/>

            <sound template="clackVar1" startTime="0.19" volumeScale="1.8" pitchScale="1.2" linkNode="attacherJoint"/>
            <sound template="clackVar5" startTime="0.19" volumeScale="1.8" pitchScale="1.4" linkNode="attacherJoint"/>
        </animation>
        <animation name="lockDrawbarLimit">
            <part componentJointIndex="1" startTime="0" endTime="0.1" startRotLimit="0 120 0" endRotLimit="0 0 0" />
            <part componentJointIndex="2" startTime="0" endTime="0.1" startRotLimit="0 0 27" endRotLimit="0 0 0" />
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="axis" playSound="true"  wheelIndices="1 2">
                <dependentPart node="axisHydraulic" />
            </movingTool>
            <movingTool node="wheelsRot" wheelIndices="1 2"/>
            <movingTool node="backDoor" playSound="true">
                <dependentPart node="backDoorHydraulic" />
            </movingTool>

            <movingTool node="drawbarPhysics" playSound="true">
                <controls axis="AXIS_DRAWBAR" invertAxis="false" mouseSpeedFactor="0.6" iconName="DRAWBAR_ROTATE_X" />
                <rotation rotSpeed="20" rotAcceleration="60" rotMax="45" rotMin="-45" startRot="0" rotationAxis="2"/>
                <inputAttacherJoint value="true" />
                <componentJoint index="1" anchorActor="0" />
            </movingTool>

            <movingTool node="pickupFlap" playSound="true">
                <controls axis="AXIS_DOOR" invertAxis="true" mouseSpeedFactor="0.6" iconName="TOP_DOOR_ROTATE" />
                <rotation rotSpeed="20" rotAcceleration="100" rotMax="0" rotMin="-70" startRot="-45" rotationAxis="1"/>
                <dependentPart node="pickupFlapHydraulic" />
                <dependentPart node="chainsFrontRow" />
                <dependentPart node="chainsBackRow" />
            </movingTool>
        </movingTools>

        <movingParts>
            <movingPart node="axisHydraulic" referencePoint="axisHydraulicRef" referenceFrame="axisHydraulicRef" >
                <translatingPart node="axisHydraulicPunch"/>
            </movingPart>

            <movingPart node="backDoorHydraulic" referencePoint="backDoorHydraulicRef" referenceFrame="backDoorHydraulicRef" >
                <translatingPart node="backDoorHydraulicPunch"/>
            </movingPart>
            <movingPart node="pickupFlapHydraulic" referencePoint="pickupFlapHydraulicRef" referenceFrame="pickupFlapHydraulicRef">
                <translatingPart node="pickupFlapHydraulicPunch" />
            </movingPart>

            <movingPart node="chainsFrontRow" referenceFrame="pickupFlap" doDirectionAlignment="false" moveToReferenceFrame="false" doRotationAlignment="true" rotMultiplier="-1"/>
            <movingPart node="chainsBackRow" referenceFrame="pickupFlap" doDirectionAlignment="false" moveToReferenceFrame="false" doRotationAlignment="true" rotMultiplier="-1"/>

            <movingPart node="drawbar" referencePoint="drawbarRef" referenceFrame="wb55_main_component1" isActiveDirty="true" maxUpdateDistance="100" limitedAxis="2" directionThreshold="0.01">
                <dependentPart node="drawbarLeftHydraulic" />
                <dependentPart node="drawbarRightHydraulic" />
                <powerTakeOffs localIndices="1 2"/>
            </movingPart>

            <movingPart node="drawbarLeftHydraulic" referencePoint="drawbarLeftHydraulicRef" referenceFrame="drawbarLeftHydraulicRef" >
                <translatingPart node="drawbarLeftHydraulicPunch"/>
            </movingPart>
            <movingPart node="drawbarRightHydraulic" referencePoint="drawbarRightHydraulicRef" referenceFrame="drawbarRightHydraulicRef" >
                <translatingPart node="drawbarRightHydraulicPunch"/>
            </movingPart>
        </movingParts>

         <sounds>
            <actionSound template="defaultHydraulicSound" pitchScale="0.8" volumeScale="1.6" linkNode="pickupFlapHydraulic"  actionNames="TRANSLATING_POS TRANSLATING_NEG" nodes="pickupFlapHydraulic" fadeIn="0.1"/>
            <actionSound template="hydraulicLayerTone"    pitchScale="1"   volumeScale="1"   linkNode="drawbarLeftHydraulic" actionNames="TRANSLATING_POS TRANSLATING_NEG" nodes="drawbarLeftHydraulic" fadeIn="0.1"/>
            <actionSound template="sheetImp01"            pitchScale="0.3" volumeScale="1.3" linkNode="pickupFlap"           actionNames="TOOL_MOVE_END_POS_LIMIT"         nodes="pickupFlap"/>
            <actionSound template="clackVar6"             pitchScale="1"   volumeScale="0.9" linkNode="pickupFlap"           actionNames="TOOL_MOVE_END_POS_LIMIT"         nodes="pickupFlap"/>
            <actionSound template="clackVar6"             pitchScale="1"   volumeScale="0.9" linkNode="pickupFlap"           actionNames="TOOL_MOVE_END_NEG_LIMIT"         nodes="pickupFlap"/>
            <actionSound template="clackVar4"             pitchScale="0.8" volumeScale="0.6" linkNode="pickupFlap"           actionNames="TOOL_MOVE_END_NEG_LIMIT"         nodes="pickupFlap"/>
            <actionSound template="foldStartMed02"        pitchScale="0.8" volumeScale="2.2" linkNode="drawbarPhysics"       actionNames="TOOL_MOVE_END_NEG_LIMIT"         nodes="drawbarPhysics"/>
            <actionSound template="clackVar4"             pitchScale="0.8" volumeScale="1.3" linkNode="drawbarPhysics"       actionNames="TOOL_MOVE_END_POS_LIMIT"         nodes="drawbarPhysics"/>
            <actionSound template="foldStartMed02"        pitchScale="0.8" volumeScale="2"   linkNode="drawbarPhysics"       actionNames="TOOL_MOVE_END_POS_LIMIT"         nodes="drawbarPhysics"/>
        </sounds>
